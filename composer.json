{
  "name": "cssjockey/laravel-svg",
  "description": "This package enables a directive @svg(..) which renders the SVG code from the given path or public URL.",
  "license": "MIT",
  "authors": [
    {
      "name": "Mohit Aneja",
      "email": "mohit@cssjockey.com",
      "homepage": "https://cssjockey.com"
    }
  ],
  "homepage": "https://github.com/cssjockey/laravel-svg",
  "keywords": [
    "laravel",
    "svg",
    "laravel-svg",
    "blade-svg",
    "directives"
  ],
  "require": {
    "illuminate/support": "~7|~8"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.0",
    "orchestra/testbench": "~5|~6"
  },
  "autoload": {
    "files": [
      "src/Support/helpers.php"
    ],
    "psr-4": {
      "CSSJockey\\LaravelSvg\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CSSJockey\\LaravelSvg\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "CSSJockey\\LaravelSvg\\LaravelSvgServiceProvider"
      ],
      "aliases": {
        "LaravelSvg": "CSSJockey\\LaravelSvg\\Facades\\LaravelSvg"
      }
    }
  }
}
